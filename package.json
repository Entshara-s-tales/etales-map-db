{
  "name": "etales-db-updater",
  "description": "Update database of Etales Web DB with data mined from the newest map version",
  "version": "0.0.0",
  "author": "Dariusz Niemczyk @Palid",
  "bin": {
    "etales-db-updater": "./bin/run"
  },
  "bugs": "https://github.com/Palid/etales-db-updater/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@wowserhq/stormjs": "^0.4.1",
    "long": "^4.0.0",
    "seek-bzip": "^2.0.0",
    "tslib": "^1.14.1",
    "war3map": "^0.1.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "stdout-stderr": "^0.1.13",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/Palid/etales-db-updater",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "etales-db-updater"
  },
  "repository": "Palid/etales-db-updater",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:watch": "npm run test -- --watch",
    "version": "oclif-dev readme && git add README.md",
    "prepare": "husky install",
    "pack": "oclif-dev pack",
    "lint": "eslint . --ext .ts --config .eslintrc"
  },
  "types": "lib/index.d.ts",
  "lint-staged": {
    "*.{ts, js}": "eslint --cache --fix",
    "*.{md}": "prettier --write"
  },
  "husky": {
    "precommit": "lint-staged"
  }
}
